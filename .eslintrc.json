{
  "env": {
    "browser": "true",
    "node": "true"
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "semi": 0,
    "comma-dangle": 1,
    "react/forbid-prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "radix": 0,
    "no-confusing-arrow": 0
  }
}
